@page
@model eButler.Pages.Admin.Shippings.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Shipping[0].Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Shipping[0].Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Shipping[0].Distric)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Shipping[0].City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Shipping[0].Country)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Shipping[0].Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Shipping[0].Note)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Shipping[0].HouseKeeper)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Shipping) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Distric)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Country)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Note)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HouseKeeper.Id)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
