@page
@model eButler.Pages.EditProfileSupplierModel
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Pages/Admin/Shared/_AdminLayout.cshtml";
}
<div class="content-wrapper">
    <div class="row">
        <div class="col-md-6 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Edit Your Profile</h4>
                    @*<p class="card-description">
                    User
                    </p>*@

                    <form method="post">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input type="hidden" asp-for="Supplier.Id" />
                        <div class="form-group">
                            <label asp-for="Supplier.Name" class="control-label"></label>
                            <input asp-for="Supplier.Name" class="form-control" />
                            <span asp-validation-for="Supplier.Name" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Supplier.Country" class="control-label"></label>
                            <input asp-for="Supplier.Country" class="form-control" />
                            <span asp-validation-for="Supplier.Country" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Supplier.Phone" class="control-label"></label>
                            <input asp-for="Supplier.Phone" class="form-control" />
                            <span asp-validation-for="Supplier.Phone" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Supplier.Disciption" class="control-label"></label>
                            <input asp-for="Supplier.Disciption" class="form-control" />
                            <span asp-validation-for="Supplier.Disciption" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Supplier.Image" class="control-label"></label>
                            <input asp-for="Supplier.Image" class="form-control" />
                            <span asp-validation-for="Supplier.Image" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <input type="submit" value="Save" class="btn btn-primary" />
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
<div>
    <a asp-page="./ProfileSupplier">Back to Your Profile</a>
</div>



